{
    "home": "~/.codalab",
    "client": {
        "class": "LocalBundleClient"
    },
    "cli": {
        "verbose": 1
    },
    "editor": "vim",
    "server": {
        "host": "localhost",
        "port": 2800,
        "class": "MySQLModel",
        "engine_url": "mysql://{{BUNDLE_DB_USER}}:{{BUNDLE_DB_PASSWORD}}@localhost:3306/{{BUNDLE_DB_NAME}}",
        "verbose": 1,
        "auth": {
            "class": "OAuthHandler",
            "address": "{{BUNDLE_AUTH_URL}}",
            "app_id": "{{BUNDLE_APP_ID}}",
            "app_key": "{{BUNDLE_APP_KEY}}"
        }
    }
}
